{
  "name": "mut_front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "cordova": "npm run build-spa && npm run replace-relative-paths && npm run copy-cordova && npm run cordova-build;",
    "build-spa": "nuxt build --spa && npm run generate",
    "replace-relative-paths": "replace '/nuxtfiles/' 'nuxtfiles/' dist --recursive && replace '/assets/' 'nuxtfiles/' dist --recursive && replace '(href|src|\"href\"|\"src\")=\"/' '$1=\"../' dist --recursive && replace '(href|src|\"href\"|\"src\"):\"/' '$1:\"../' dist --recursive",
    "replace-relative-paths-win": "node ./scripts/replace-win.js",
    "clear-cordova-win": "rmdir /s/q cordova\\www && mkdir cordova\\www",
    "copy-cordova-win": "xcopy /E /I /Y dist cordova\\www",
    "cordova-win": "npm run build-spa && npm run replace-relative-paths-win && npm run copy-cordova-win && npm run cordova-build",
    "copy-cordova": "cp -R dist/* cordova/www/",
    "clear-cordova": "rm -R cordova/www/*",
    "cordova-build": "cd cordova && cordova build",
    "run-android": "cd cordova && cordova run android"
  },
  "dependencies": {
    "@capacitor/android": "^4.6.2",
    "@capacitor/app": "^4.1.1",
    "@capacitor/core": "^4.6.2",
    "@capacitor/ios": "^4.6.2",
    "@capacitor/local-notifications": "^4.1.4",
    "@chenfengyuan/vue-number-input": "^1.2.1",
    "@mdi/font": "^7.2.96",
    "@nuxtjs/auth-next": "5.0.0-1648802546.c9880dc",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/i18n": "^7.3.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/svg": "^0.4.0",
    "@nuxtjs/toast": "^3.3.1",
    "core-js": "^3.15.1",
    "debounce": "^1.2.1",
    "load-google-maps-api": "^2.0.2",
    "material-icons": "^1.13.1",
    "nuxt": "^2.15.7",
    "postcss": "^8.4.26",
    "pusher-js": "^8.0.0",
    "qs": "^6.11.0",
    "uuid": "^9.0.0",
    "vue-cli-plugin-vuetify-preset-reply": "^1.0.4",
    "vue-scroll-picker": "^0.8.2",
    "vue-slick-carousel": "^1.0.6",
    "vue-slick-carousel-rtl": "^1.0.8",
    "vue-tel-input-vuetify": "^1.5.3",
    "vue2-google-maps": "^0.10.7",
    "vue2-timepicker": "^1.1.6",
    "vuelidate": "^0.7.7",
    "vuelidate-messages": "^0.1.2",
    "vuetify": "^2.5.5",
    "vuetify-confirm": "^2.0.6",
    "vuex-map-fields": "^1.4.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.6.2",
    "@nuxt/types": "~2.17.0",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/vuetify": "^1.12.1",
    "replace": "^1.1.0"
  }
}
